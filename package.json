{
  "name": "tabla-frontend",
  "version": "0.0.0",
  "description": "Frontend of Tabla webapp",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "start": "vite",
    "start:prod": "vite preview",
    "build": "vite build",
    "prettier": "npx prettier --write --config .prettierrc.json --ignore-path .prettierignore .",
    "eslint": "npx eslint -c .eslintrc.json --ignore-path .eslintignore .",
    "test": "jest --testPathIgnorePatterns=./test/e2e",
    "test:watch": "jest --testPathIgnorePatterns=./test/e2e --watch",
    "test:cov": "jest --testPathIgnorePatterns=./test/e2e --coverage",
    "test:e2e": "jest --testPathPattern=./test/e2e",
    "test:e2e:cov": "jest --testPathPattern=./test/e2e --coverage"
  },
  "dependencies": {
    "solid-js": "^1.7.7"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.22.7",
    "@solidjs/testing-library": "^0.8.4",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^29.5.2",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "babel-preset-solid": "^1.7.7",
    "eslint": "^8.44.0",
    "eslint-plugin-solid": "^0.12.1",
    "jest": "^29.6.0",
    "jest-css-modules-transform": "^4.4.2",
    "jest-environment-jsdom": "^29.6.0",
    "prettier": "^2.8.8",
    "ts-jest": "^29.1.1",
    "typescript": "^5.1.6",
    "vite": "^4.3.9",
    "vite-plugin-solid": "^2.7.0"
  }
}
